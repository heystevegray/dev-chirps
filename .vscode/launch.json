	"version": "0.2.0",
	"configurations": [
		{
			"name": "Attach to Client",
			"request": "launch",
			"type": "pwa-chrome",
			"url": "http://localhost:3000",
			"webRoot": "${workspaceFolder}"
		},
		{
			"command": "cd server && npm run dev",
			"name": "Run npm dev",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"command": "cd server && npm run dev:gateway",
			"name": "Run npm dev:gateway",
			"request": "launch",
			"type": "node-terminal"
		},
	]
}